 {% extends 'NineThousandJobqueueBundle::layout.html.twig' %}
 {% block title "Jobqueue History" %}
 {% block body %}
 
 <section id="History" />
    <h1>Jobqueue History</h1>
    <article id="jobhistory">
    {% if history.totalEntries %}
            <ul>
            {% for entry in history %}
                <li>
                    <ul class="jobdata">
                        <lh> Job id </lh>
                        <li> {{ entry.jobId }} </li>
                        <lh> Job Type </lh>
                        <li> {{ entry.jobType }} </li>
                        <lh> Status </lh>
                        <li> {{ entry.status }} </li>
                        <lh> Message </lh>
                        <li> {{ entry.message }} </li>
                        <lh> Job Schedule </lh>
                        <li> {{ entry.jobSchedule }} </li>
                        <lh> Timestamp </lh>
                        <li> {{ entry.timestamp|date("m/d/Y") }} </li>
                    </ul>
                </li>
            {% endfor %}
            </ul>
        {% else %}
            <p class="emptyhistory"> No jobs found in history </p>
        {% endif %}
    </article>
 </section>
 {% endblock %}
