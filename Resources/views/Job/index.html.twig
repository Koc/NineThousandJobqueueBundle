<h1>Job list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Retry</th>
            <th>Cooldown</th>
            <th>Maxretries</th>
            <th>Attempts</th>
            <th>Executable</th>
            <th>Type</th>
            <th>Status</th>
            <th>Createdate</th>
            <th>Lastrundate</th>
            <th>Active</th>
            <th>Schedule</th>
            <th>Parent</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('jobqueue_job_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.name }}</td>
            <td>{{ entity.retry }}</td>
            <td>{{ entity.cooldown }}</td>
            <td>{{ entity.maxRetries }}</td>
            <td>{{ entity.attempts }}</td>
            <td>{{ entity.executable }}</td>
            <td>{{ entity.type }}</td>
            <td>{{ entity.status }}</td>
            <td>{{ entity.createDate|date('Y-m-d H:i:s') }}</td>
            <td>{{ entity.lastrunDate|date('Y-m-d H:i:s') }}</td>
            <td>{{ entity.active }}</td>
            <td>{{ entity.schedule }}</td>
            <td>{{ entity.parent }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('jobqueue_job_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('jobqueue_job_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('jobqueue_job_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
