<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="jobqueue_tag" pattern="/">
        <default key="_controller">NineThousandJobqueueBundle:Tag:index</default>
    </route>

    <route id="jobqueue_tag_show" pattern="/{id}/show">
        <default key="_controller">NineThousandJobqueueBundle:Tag:show</default>
    </route>

    <route id="jobqueue_tag_new" pattern="/new">
        <default key="_controller">NineThousandJobqueueBundle:Tag:new</default>
    </route>

    <route id="jobqueue_tag_create" pattern="/create">
        <default key="_controller">NineThousandJobqueueBundle:Tag:create</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="jobqueue_tag_edit" pattern="/{id}/edit">
        <default key="_controller">NineThousandJobqueueBundle:Tag:edit</default>
    </route>

    <route id="jobqueue_tag_update" pattern="/{id}/update">
        <default key="_controller">NineThousandJobqueueBundle:Tag:update</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="jobqueue_tag_delete" pattern="/{id}/delete">
        <default key="_controller">NineThousandJobqueueBundle:Tag:delete</default>
        <requirement key="_method">post</requirement>
    </route>

</routes>
