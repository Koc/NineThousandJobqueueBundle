<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="jobqueue_arg" pattern="/">
        <default key="_controller">NineThousandJobqueueBundle:Arg:index</default>
    </route>

    <route id="jobqueue_arg_show" pattern="/{id}/show">
        <default key="_controller">NineThousandJobqueueBundle:Arg:show</default>
    </route>

    <route id="jobqueue_arg_new" pattern="/new">
        <default key="_controller">NineThousandJobqueueBundle:Arg:new</default>
    </route>

    <route id="jobqueue_arg_create" pattern="/create">
        <default key="_controller">NineThousandJobqueueBundle:Arg:create</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="jobqueue_arg_edit" pattern="/{id}/edit">
        <default key="_controller">NineThousandJobqueueBundle:Arg:edit</default>
    </route>

    <route id="jobqueue_arg_update" pattern="/{id}/update">
        <default key="_controller">NineThousandJobqueueBundle:Arg:update</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="jobqueue_arg_delete" pattern="/{id}/delete">
        <default key="_controller">NineThousandJobqueueBundle:Arg:delete</default>
        <requirement key="_method">post</requirement>
    </route>

</routes>
