<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="jobqueue_param" pattern="/">
        <default key="_controller">NineThousandJobqueueBundle:Param:index</default>
    </route>

    <route id="jobqueue_param_show" pattern="/{id}/show">
        <default key="_controller">NineThousandJobqueueBundle:Param:show</default>
    </route>

    <route id="jobqueue_param_new" pattern="/new">
        <default key="_controller">NineThousandJobqueueBundle:Param:new</default>
    </route>

    <route id="jobqueue_param_create" pattern="/create">
        <default key="_controller">NineThousandJobqueueBundle:Param:create</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="jobqueue_param_edit" pattern="/{id}/edit">
        <default key="_controller">NineThousandJobqueueBundle:Param:edit</default>
    </route>

    <route id="jobqueue_param_update" pattern="/{id}/update">
        <default key="_controller">NineThousandJobqueueBundle:Param:update</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="jobqueue_param_delete" pattern="/{id}/delete">
        <default key="_controller">NineThousandJobqueueBundle:Param:delete</default>
        <requirement key="_method">post</requirement>
    </route>

</routes>
