<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="ninethousand_jobqueue_home" pattern="/">
        <default key="_controller">NineThousandJobqueueBundle:Default:index</default>
    </route>

    <import resource="@NineThousandJobqueueBundle/Resources/config/routing/history.xml" prefix="/history" />
    <import resource="@NineThousandJobqueueBundle/Resources/config/routing/job.xml" prefix="/job" />
    <import resource="@NineThousandJobqueueBundle/Resources/config/routing/scheduledjob.xml" prefix="/scheduledjob" />
    <import resource="@NineThousandJobqueueBundle/Resources/config/routing/arg.xml" prefix="/arg" />
    <import resource="@NineThousandJobqueueBundle/Resources/config/routing/param.xml" prefix="/param" />
    <import resource="@NineThousandJobqueueBundle/Resources/config/routing/tag.xml" prefix="/tag" />
</routes>
